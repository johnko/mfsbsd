#!/bin/sh

/usr/bin/tr -d '\r' | /usr/bin/sed \
	-e 's#[^:_ ]\{5\}:#_____:#g' -e 's#:[^:_ ]\{5\}#:_____#g' \
	-e 's#[^:_ ]\{4\}:#____:#g'  -e 's#:[^:_ ]\{4\}#:____#g' \
	-e 's#[^:_ ]\{3\}:#___:#g'   -e 's#:[^:_ ]\{3\}#:___#g' \
	-e 's#[^:_ ]\{2\}:#__:#g'    -e 's#:[^:_ ]\{2\}#:__#g' \
	-e 's#[^:_ ]:#_:#g'          -e 's#:[^:_ ]#:_#g' \
	-e 's#:_____#:____ #g' -e 's#:____#:___ #g' -e 's#:___#:__ #g' \
	-e 's#___:__ #__:__  #g' -e 's#__:___$#__:__#g' \
	-e 's#[0-9_]\{3\}\.[0-9_]#___._#g' -e 's#[0-9_]\.[0-9_]\{3\}#_.___#g' \
	-e 's#[0-9_]\{2\}\.[0-9_]#__._#g'  -e 's#[0-9_]\.[0-9_]\{2\}#_.__#g' \
	-e 's#[0-9_]\.[0-9_]#_._#g'
