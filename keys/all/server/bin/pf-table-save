#!/bin/sh
if [ "$( id -u )" != "0" ]; then
	echo "ERROR: Must be root!"
	exit 1
fi

if [ -z "$1" ]; then
	echo "Usage: ${0##*/} table"
	exit 1
fi


if /sbin/mount | /usr/bin/grep "/server/savepf" > /dev/null 2> /dev/null; then


	outfile=/server/savepf/$1.table
	/sbin/pfctl -P -t $1 -T show \
		>${outfile} \
		2>/dev/null

fi

